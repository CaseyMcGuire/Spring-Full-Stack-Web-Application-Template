{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "src/main/web-frontend",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "allowJs": false,
    "allowSyntheticDefaultImports": true
  }
}